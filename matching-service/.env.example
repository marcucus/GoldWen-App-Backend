# GoldWen Matching Service - Environment Configuration

# API Authentication
API_KEY=matching-service-secret-key

# Database Configuration (PostgreSQL)
# Format: postgresql://username:password@host:port/database
DATABASE_URL=postgresql://goldwen_user:goldwen_password@localhost:5432/goldwen_db

# Redis Configuration (Optional - for caching)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
CACHE_TTL=3600
CACHE_ENABLED=true

# Server Configuration
HOST=0.0.0.0
PORT=8000

# Logging
LOG_LEVEL=INFO
